// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Schedule {
	id    String     @id @default(uuid())
  name   String
  description String?
  active Boolean

  cron   String?

  year String?
	month String?
	date String?
	dayOfWeek String?
	hour  String?
	minute String?
	second String?
	tz String?

	script String?
}
